#include<iostream>       
using namespace std;      
int main()        
{            
    long long int n=0,k=0,a=3,b=0,c=5,d=0,e=0;    
     
    while(cin>>n){    
            
            
          while(n>=1)  
          {  
            k+=1;  
            b=a*k;  
            d=c*k;  
            e+=k*b*d;         
            if(k==n){  
                break;  
            }       
          }     
          cout<<"F="<<e<<endl;       
    }    
        
    return 0;        
}         

