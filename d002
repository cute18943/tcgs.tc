#include<iostream>       
#include<cctype>       
using namespace std;        
int main()          
{              
    int a,b,c,d;      
    char e[9999];      
        
       cin>>a>>e[9999]>>b>>e[9999]>>c>>e[9999]>>d;  
               
    if(a>=1 && b>=0&&c>=0&&d>=0 && a<=126 && b<=255 && c<=255 && d<=255){      
        cout<<"A"<<endl;      
    }else if(a>=128 && b>=0&&c>=0&&d>=0 && a<=191 && b<=255 && c<=255 && d<=255){      
        cout<<"B"<<endl;      
    }else if(a>=192 && b>=0&&c>=0&&d>=0 && a<=223 && b<=255 && c<=255 && d<=255){      
        cout<<"C"<<endl;      
    }else if(a>=224 && b>=0&&c>=0&&d>=0 && a<=239 && b<=255 && c<=255 && d<=255){      
        cout<<"D"<<endl;      
    }else if(a>=240 && b>=0&&c>=0&&d>=0 && a<=247 && b<=255 && c<=255 && d<=255){      
        cout<<"E"<<endl;      
    }else if(a==127 && b==0&&c==0&&d==1  ){      
        cout<<"X"<<endl;      
    }else if(a==255 && b<=255 && c<=255 && d<=255){      
        cout<<"X"<<endl;      
    }else if(isdigit(a) &&isdigit(b) &&isdigit(c)&&isdigit(d)){      
           
    } else{    
            cout<<"N"<<endl;    
        }        
    return 0;          
}           

