#include <iostream>    
    
using namespace std;    
int main() {    
    
        
    int n,num[50],m=0,p=0;    
    while(cin>>n){    
          
        for(int i=0;i<n;i++){    
            cin>>num[i];    
           
                
       if(num[i]%2==0){  
        m+=1;  
       }  
         
       if(num[i]%2==1){  
        p+=1;  
       }  
       }   
       cout<<p<<" "<<m<<endl;  
    }    
    return 0;    
}    

