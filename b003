#include <iostream>  
  
using namespace std;  
int main() {  
  
      
    int n,num[15],m;  
    while(cin>>n){  
        int big=0,small=0;  
        for(int i=0;i<n;i++){  
            cin>>num[i];  
        }  
              
        cin>>m;  
        for(int i=0;i<n;i++){  
            if(num[i]>m)big++;  
            else if(num[i]<m)small++;  
        }  
        cout<<big<<' '<<small<<'\n';  
    }  
    return 0;  
}  

