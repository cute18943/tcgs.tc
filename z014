#include<iostream>  
using namespace std;  
int main()    
{        
    int n;   
      
    while(cin>>n){  
      
        if(n>=1)  
        {  
            cout<<"yes"<<endl;  
        }else  
        {  
            cout<<"no"<<endl;  
        }  
          
    }  
      
    return 0;    
}     

