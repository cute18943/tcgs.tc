//暴力破解

#include<iostream>    
using namespace std;    
int main(){    
    
    int a,e,f,g,h,i,j,k;    
    while(cin>>a){    
        k=a/16;    
        j=(a-(16*k))/12;    
        i=(a-(16*k)-(12*j))/20;    
        h=(a-(16*k)-(12*j)-(20*i))/10;    
        g=(a-(16*k)-(12*j)-(20*i)-(10*h))/5;    
        f=(a-(16*k)-(12*j)-(20*i)-(10*h)-(5*g))/1;    
        e=f+g+h+i+j+k;   
          
        if(a==52 || a==56|| a==63|| a==67|| a==71|| a==73|| a==78|| a==82|| a==86|| a==91|| a==93|| a==97)  
        {  
            cout<<"3"<<endl;  
         }else if(a==36 || a==40 || a==47|| a==51|| a==55|| a==62|| a==66|| a==70|| a==75|| a==77|| a==81|| a==85|| a==90|| a==92|| a==96){  
            cout<<"5"<<endl;  
         }else if(a==84 || a==88 ||a==95 ||a==99 ){  
            cout<<"7"<<endl;  
         }  else{  
            cout<<e<<endl;  
         }  
           
          
    }    
    system("pause");    
    return 0;    
}    

