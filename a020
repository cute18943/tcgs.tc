#include<iostream>    
using namespace std;    
    
int main()    
{       
    int n,x,y=0;    
    cin >> n >>x ;    
    if( x==2 || x==5 || x==8){      
        y+=200;  
        if(n%2==1 ){  
              
                y+=100;  
            if(n==5 && x==5){  
                y+=50;  
                cout<<y<<endl;  
            }else  
            {  
              
            cout<<y<<endl;  
            }    
        }else  
        {     
            if(n==x){  
                y+=50;  
                cout<<y<<endl;         
            }  
            else  
            {  
                cout<<y<<endl;  
            }  
        }  
    }    
    else if(n%2==1){    
        y+=100;  
        if( x==2 || x==5 || x==8){  
            y+=200;  
             cout<<y<<endl;    
        }else{    
            if(n==x){  
                y+=50;  
                cout<<y<<endl;    
            }else  
            {  
                cout<<y<<endl;  
            }          
        }     
    }    
    else if(n==4 && x==4)  
    {  
        y+=50;  
        cout<<y<<endl;  
    }else if(n==6 && x==6){  
        y+=50;  
        cout<<y<<endl;  
    }  
    else    
    {    
        y=0;  
        cout<<y<<endl;    
    }    
    return 0;    
}     

