#include<iostream>  
#include<iomanip>  
using namespace std;  
  
int main()  
{  
      
  
      
    double x,n;  
    cin >> n ;  
    x=n*9/5+32;  
    cout<<setiosflags(ios::fixed)<<setprecision(1) << x ;  
      
    return 0; 
}   

