#include<iostream>  
using namespace std;  
int main()    
{        
    int n,x,z;   
    int a,b;   
    while( cin>>n>>x>>z){  
       
     if(n==7)  
     {  
          
        a=x+z;  
        if(a<=61)  
        cout<<"YES"<<endl;  
        else  
        cout<<"NO"<<endl;     
     }else  
     {  
        a=x+z;  
        if(a<=30)  
        cout<<"YES"<<endl;  
        else  
        cout<<"NO"<<endl;     
     }   
     }  
    return 0;    
}     

