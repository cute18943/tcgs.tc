#include<iostream>    
    
using namespace std;    
int main()      
{          
    long long int n,x,y;     
        
    while(cin>>n>>x){    
         
         if(n<=x)  
         {  
      
            if(n==0 && x==0)  
            {  
                break;  
             }else  
             {  
                cout<<"Fair"<<endl;  
             }  
              
         }else  
         {  
            cout<<"Unfair"<<endl;  
         }  
           
           
    }    
        
    return 0;      
}       

