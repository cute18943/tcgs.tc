#include<iostream>  
using namespace std;  
int main()    
{        
    int n,x,i,j;    
    cin>>n>>x;    
    
  for(i=1;i<=x;i++){    
     for(j=1;j<=n;j++){    
        cout<<"#";  
       }    
    cout<<endl;    
  }         
    return 0;    
}     

