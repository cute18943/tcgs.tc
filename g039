#include <iostream>    
    
using namespace std;    
int main() {    
    
    int a,b[10],c[10],d[10],i;  
  
    cin>>a;  
      
    for(i=1;i<=a;i++){  
        cin>>b[i]>>c[i]>>d[i];  
    }  
      
    for(i=1;i<=a;i++){  
    if(b[i]!=c[i] && c[i]!=d[i] && b[i]!=d[i] ){  
        cout<<"YES"<<endl;  
    }else{  
        cout<<"NO"<<endl;  
    }  
    }  
    return 0;    
}    

