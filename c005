#include <iostream>        
    
using namespace std;        
int main() {        
        
    int a,b,c[21],d[21],h=0,i=0;  
    double e=0,f=0;  
      
    cin>>a;  
    for(b=1;b<=a;b++){  
        cin>>c[b];  
        e+=c[b];  
    }  
      
     for(b=1;b<=a;b++){  
        cin>>d[b];  
        f+=d[b];  
    }  
      
    e=e/a;  
    f=f/a;  
      
    for(b=1;b<=a;b++){  
        h+=1;  
        if(c[b]>=e && d[b]>=f){  
            cout<<b<<" ";  
        }else{  
            i+=1;  
        }  
    }  
        if(h-i==0){  
            cout<<"NO"<<endl;  
        }  
    return 0;        
}        

