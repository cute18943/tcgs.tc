#include<iostream>    
using namespace std;    
int main()      
{          
    int n;     
        
    while( cin>>n){    
      n+=1;  
    if(n>=0 && n<=20)    
    {    
        cout<<"60"<<endl;    
    }else if(n>=21 && n<=50)    
    {    
        cout<<"80"<<endl;    
    }else if(n>=51 && n<=100)    
    {    
        cout<<"90"<<endl;    
    }else if(n>=101)    
    {    
        cout<<"100"<<endl;    
    }    
          
}    
    return 0;      
}       


