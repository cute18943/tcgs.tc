#include <iostream>      
using namespace std;      
      
int main()      
{      
    int x,y,z;      
    while(cin>>x>>y>>z){    
            
        if(x>y+z){cout<<"甲"<<endl;    
        }    
        else if(y>x+z){cout<<"乙"<<endl;    
        }    
        else if(z>y+x){cout<<"丙"<<endl;    
        }    
        else if(x>y && x<z){cout<<"甲"<<endl;      
        }      
              
        else if(x>z && x<y){cout<<"甲"<<endl;      
        }      
              
        else if(y>x && y<z){cout<<"乙"<<endl;      
        }      
              
        else if(y>z && y<x){cout<<"乙"<<endl;      
        }      
              
        else if(z>x && z<y){cout<<"丙"<<endl;      
        }      
              
        else if(z>y && z<x){cout<<"丙"<<endl;      
        }      
    }  
       
 return 0;      
}      

