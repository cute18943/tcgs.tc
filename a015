#include<iostream>  
using namespace std;  
  
int main()  
{     
    int n,x;  
    cin >> n >> x ;  
    if(n==x)  
    {  
        cout<<n<<"="<<x;  
    }  
    else if(n>x)  
    {  
        cout<<n<<">"<<x;  
    }  
    else if(n<x)  
    {  
        cout<<n<<"<"<<x;  
    }  
     
    return 0;  
}   

