#include<iostream>    
    
using namespace std;    
    
int main(){    
        
    int a,b,c[85],d=0,e;    
        
    cin>>a;    
        
    for(b=1;b<=a;b++){    
        cin>>c[b];    
    }    
        
    for(b=1;b<=a;b++){    
        if(c[b]<=25){    
            d+=1;    
        }    
    }    
        
    if(d>=5){    
        d=5;    
        cout<<d<<endl;    
    }    
    if(d==4){    
        d=4;    
        cout<<d<<endl;    
    }    
    if(d==3){    
        d=3;    
        cout<<d<<endl;    
    }    
    if(d==2){    
        d=2;    
        cout<<d<<endl;    
    }    
    if(d==1){    
        d=1;    
        cout<<d<<endl;    
    }    
        
     if(d==0){    
        d=0;    
        cout<<d<<endl;    
    }      
        
    return 0;    
}     

