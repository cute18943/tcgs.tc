#include<iostream>      
#include<cmath>    
#include<iomanip>    
using namespace std;      
int main()        
{            
     double x1=0,y1=0,x2=0,y2=0,a=0,b=0;       
          
    while(cin>>x1>>y1>>x2>>y2){      
    a = (x1 > x2 ? x1 - x2 : x2 - x1);  
    b = (y1 > y2 ? y1 - y2 : y2 - y1);  
               
        cout<<setiosflags(ios::fixed)<<setprecision(3) << sqrt(a*a+b*b)<<endl ;       
       }       
    return 0;        
}         

