#include <iostream>  
  
using namespace std;  
int main() {  
  
    int n;  
    while(cin>>n){  
        int maxn=0,maxp,num;  
        for(int i=1;i<=n;i++){  
            cin>>num;  
            if(num>maxn){  
                maxn=num;  
                maxp=i;  
            }  
        }  
        cout<<maxp<<' '<<maxn<<'\n';  
    }  
    return 0;  
}  

