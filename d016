#include<iostream>  
using namespace std;  
int main()    
{        
    int n,x;   
      
    while(cin>>n>>x){  
       
     if(n==0 && x==0)  
     {  
        cout<<"("<<n<<","<<x<<")在原點"<<endl;   
        }   
     else if(n==0 && x>0)   
      {  
        cout<<"("<<n<<","<<x<<")在Y軸"<<endl;   
        }  
    else if(n==0 && x<0)   
      {  
        cout<<"("<<n<<","<<x<<")在Y軸"<<endl;   
        }  
       else if(n>0 && x==0)   
      {  
        cout<<"("<<n<<","<<x<<")在X軸"<<endl;   
        }  
    else if(n<0 && x==0)   
      {  
        cout<<"("<<n<<","<<x<<")在X軸"<<endl;   
        }   else if(n<0 && x<0)   
      {  
        cout<<"("<<n<<","<<x<<")在第三象限"<<endl;   
        }   else if(n>0 && x>0)   
      {  
        cout<<"("<<n<<","<<x<<")在第一象限"<<endl;   
        }   else if(n<0 && x>0)   
      {  
        cout<<"("<<n<<","<<x<<")在第二象限"<<endl;   
        }   else if(n>0 && x<0)   
      {  
        cout<<"("<<n<<","<<x<<")在第四象限"<<endl;   
        }  
    }  
      
    return 0;    
}     

