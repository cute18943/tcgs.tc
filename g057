#include <iostream>    
using namespace std;    
    
int main()    
{    
 int n=0,s=0,i=0,j=0,k=0,t=0,x=0,y=0,z=0,a,b;    
    
  cin>>a;  
  for(b=1;b<=a;b++){  
   
   cin>>n;  
   while(n)    
 {    
  s=n%10;    
  n=(n-s)/10;    
  i+=s;   
    
  j=i/10;    
  k=i%10;    
  t=j+k;    
      
  x=t/10;    
  y=t%10;    
  z=x+y;    
 }    
  if(z==2){  
    cout<<z<<", Yes"<<endl;   
  }else  
  {  
    cout<<z<<", No"<<endl;   
  }   
  i=0;    
 }   
 return 0;    
}    

