#include<iostream>    
using namespace std;    
int main()      
{          
    int n,x,y,z;     
        
    while(cin>>n>>x){    
         
         if(n==0 && x==0)    
          {    
            break;    
          }    
            else if(n%2==1 && x%2==1){    
            cout<<"Win"<<endl;    
          } else if(n%2==0 && x%2==0){    
            cout<<"Win"<<endl;    
          }  else{  
            cout<<"Loss"<<endl;    
          }  
    }    
        
    return 0;      
}       

