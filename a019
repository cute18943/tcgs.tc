#include<iostream>  
using namespace std;  
  
int main()  
{     
    int n,x;  
    cin >> n >>x ;  
    if( x==2 || x==5 || x==8){  
        cout<<"200"<<endl;  
    }  
    
    else if(n%2==1){  
        cout<<"100"<<endl;  
    }  
    else if(n==x){  
        cout<<"50"<<endl;  
    }else  
    {  
        cout<<"0"<<endl;  
    }  
      
    return 0;  
}   

