#include<iostream>  
using namespace std;  
  
int main()  
{     
    int n,x;  
    cin >> n >>x ;  
    if(n==14 && x>=20 && x<60){  
        cout<<"YES"<<endl;  
    }  
      else if(n==15 && x>=0 && x<60)  
    {  
        cout<<"YES"<<endl;  
    } else if(n==16 && x>=0 && x<=40)  
    {  
        cout<<"YES"<<endl;  
    } else   
    {  
        cout<<"NO"<<endl;  
    }  
      
    return 0;  
}   

