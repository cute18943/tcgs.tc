#include <iostream>        
#include<iomanip>     
using namespace std;        
int main() {        
        
     int n=5;    
    double b=0;    
    int a[100000];      
      
          
    for(int i=0;i<n;i++){            
    cin >> a[i];      
    }             
      
    int min=a[0];                        
    for(int i=0;i<n;i++){            
        if(a[i]<min)      
        min=a[i];      
    }      
        
    int max=0;    
    for(int i=0;i<n;i++){            
        if(a[i]>max)      
        max=a[i];      
    }      
        
    for(int i=0;i<n;i++){            
        b=b+a[i];    
    }      
       
    b=(b-max-min)/3;    
        
    cout<<setiosflags(ios::fixed)<<setprecision(2) << b<<endl ;           
        
    return 0;        
}        

