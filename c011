#include <iostream>        
     
using namespace std;        
int main() {        
        
    int n,a[101],b=0,max=0;     
        
    cin>>n;    
        
    for(int i=1;i<=n;i++){    
        cin>>a[i];    
    }    
    
    for (int i = 1; i <= n; i++) {    
        if (a[i] > max){    
            max = a[i];      
        }    
    }    
        
    for (int i = 1; i <=n; i++) {    
       if (a[i] == max) {    
            continue;    
        }else if(a[i] > b){    
            b = a[i];    
        }    
    }     
        
    cout<<b<<endl;    
        
    return 0;        
}        

