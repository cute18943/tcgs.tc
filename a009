#include<iostream>  
using namespace std;  
  
int main()  
{     
    int n,x,y;  
    cin >> n ;  
     y=n/11;  
    if(n>=0)  
      {  
      cout<<(n-y)*100;      
      }       
    else  
      {  
        cout<<n*100<<endl;  
      }  
    return 0;  
}   

